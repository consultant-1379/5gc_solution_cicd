eric-data-search-engine:
  affinity:
    podAntiAffinity: hard
  autoSetRequiredWorkerNodeSysctl: false
  enabled: true
  index_management:
    delete_indices_by_diskspace:
    - index: adp-app-debug-logs*
      size: 4gb
    - index: adp-app-logs*
      size: 8gb
    - index: jaeger-span*
      size: 12gb
    enabled: true
    job_interval_in_minutes: 60
    policies:
    - description: Remove debug logs older than 3 days
      indices:
      - adp-app-debug-logs*
      priority: 1
      states:
      - name: init
        transitions:
        - condition: min_index_age=3d
          destination_state: delete
      - actions:
        - delete
        name: delete
    - description: Remove non-audit, non-debug logs older than 15 days
      indices:
      - adp-app-logs*
      priority: 1
      states:
      - name: init
        transitions:
        - condition: min_index_age=15d
          destination_state: delete
      - actions:
        - delete
        name: delete
    - description: Remove audit/alarm/security logs older than 30 days
      indices:
      - adp-app-audit-logs*
      - adp-app-asi-logs*
      - adp-app-security-logs*
      priority: 1
      states:
      - name: init
        transitions:
        - condition: min_index_age=30d
          destination_state: delete
      - actions:
        - delete
        name: delete
    - description: Remove jeager span and jeager service indices older than 8 days
      indices:
      - jaeger-span*
      - jaeger-service*
      priority: 1
      states:
      - name: init
        transitions:
        - condition: min_index_age=8d
          destination_state: delete
      - actions:
        - delete
        name: delete
  jvmHeap:
    data: 2048m
    ingest: 1024m
  nodeSelector:
    bragent: {}
    data: {}
    ingest: {}
    master: {}
  persistence:
    data:
      persistentVolumeClaim:
        size: 175Gi
        storageClassName: '{{ storageClassName|default("network-block") }}'
    master:
      persistentVolumeClaim:
        size: 16Mi
        storageClassName: {{ storageClassName|default("network-block") }}
